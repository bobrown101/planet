<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <title>Planet Node JS</title>

  <meta name="description" content="Planet Node.js rounds up the latest news from the community.">
  <meta name="keywords" content="Node.js, NodeJS, Node">
  <meta name="robots" content="index, follow" />


  <link rel="shortcut icon" href="/images/favicon.ico">


  <!-- Latest compiled and minified CSS -->
  <link href='http://fonts.googleapis.com/css?family=Open+Sans:300' rel='stylesheet' type='text/css'>
  <link rel="stylesheet" type="text/css" href="//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css">
  <link rel="stylesheet" type="text/css" href="/css/style.css">
  <link rel="stylesheet" type="text/css" href="/css/app.css">

  <link href="/type/fontello.css" rel="stylesheet">

  <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!--[if lt IE 9]>
    <script src="/js/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
  <![endif]-->

</head>
<body>
<div class="body-wrapper">
  <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
    <div class="container">
      <!-- Brand and toggle get grouped for better mobile display -->
      <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-navbar-collapse">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <a class="navbar-brand" href="#">
          
          <h1 class='logo'>PlANET NODE.JS</h1>
        </a>
      </div>

      <!-- Collect the nav links, forms, and other content for toggling -->
      <div class="collapse " id="bs-navbar-collapse">
        <ul class="nav navbar-nav navbar-right hidden-xl hidden-lg hidden-md hidden-sm">
          <li class="active"><a href="#" id="nav-option-articles" class="nav-list">Articles</a></li>
          <li class=""><a href="#" id="nav-option-sites" class="nav-list">Sites</a></li>
        </ul>

      </div><!-- /.navbar-collapse -->
    </div><!-- /.container-fluid -->
  </nav>
  <div class="light-wrapper">
    <div class="container inner">
        <div id="articles" class="col-sm-8 col-md-9 col-lg-10">
          <div class="classic-blog">
          
            <% for(var i=0; i<data.length; i++) { %>
              <div class="post well well-lg">
                <div class="post-content image-caption">
                  <div class ="quick-look">
                    <h2 class="post-title">
                      <a href="<%= data[i].link %>"><%= data[i].title %></a>
                    </h2>
                    <div class="meta">
                      <span class="date">Posted on <a href="#"><%=data[i].displayDate%></a></span>
                      <!-- <span class="category">Under <a href="#">Audio</a>, <a href="#">Inspiration</a>, <a href="#">Music</a></span> -->
                      <% if(data[i].author) { %>
                      <span class="author">Author: <a><%=data[i].author%></a></span>
                      <% } %>
                      <div
                        class="addthis_toolbox addthis_default_style addthis_16x16_style"
                        addthis:url="<%=data[i].link%>"
                        addthis:title="<%=data[i].title%>"
                      >
                        <a class="addthis_button_facebook"></a>
                        <a class="addthis_button_twitter"></a>
                        <a class="addthis_button_google_plusone_share"></a>
                        <a class="addthis_button_hackernews"></a>
                        <a class="addthis_button_compact"></a><a class="addthis_counter addthis_bubble_style"></a>
                      </div>
                    </div>
                    <!-- /.meta -->
                  </div>

                  <div class="in-depth">
                    <div class="post-isoTimestamp" style="display:none"><%=data[i].isoTimestamp%></div>
                    <div class="post-id" style="display:none"><%=data[i]._id%></div>
                    <div class="post-description"><%- data[i].content %></div>
                  </div>

                  <div class="show-me-more">
                    <a class="left">Show Me More 
                      <span href="" class="glyphicon glyphicon-chevron-right right"></span>
                    </a>
                  </div>
                </div>
                <!-- /.post-content -->
              </div>
              <!-- /.post -->
            <% } %>
          </div>
          

        </div>
        <aside id="sites" class="col-sm-4 col-md-3 col-lg-2 lp30 hidden-xs sidebar">
          <div class="sidebox widget well">
            <h2 class="widget-title">Sites</h2>
            <ul class='post-list'>
                <% for (var i = 0; i < sitesList.length; i++) {%>
                
                  <li>
                    <h6><a href="<%=sitesList[i].link%>"><%=sitesList[i].name%></a></h6>
                  </li>
                <% } %>
              
            </ul>
            <!-- /.post-list -->
          </div>
          <!-- /.widget -->
        </aside>
        <!-- /.col-sm-4 .sidebar -->


    </div>
  </div>
  <footer class="footer">
    <div class="sub-footer">
      <div class="container">
        <p class="pull-left">Built for you by the Node.js Community.</p>
        <p class="pull-right">- Contribute on <a href="https://github.com/madgloryint/planet">Github</a> -</p>
      </div>
    </div>
  </footer>
  <!-- /footer -->

</div>
<!-- /.body-wrapper -->

<!-- /canvas -->
<script src="/js/jquery.min.js"></script>
<script type="text/javascript" src="/js/hide-show-content.js"></script>
<!-- Latest compiled and minified JavaScript -->
<script src="//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js"></script>
<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
  ga('create', 'UA-33396402-5', 'planetnodejs.com');
  ga('send', 'pageview');
</script>
<script type="text/javascript">
  var addthis_config = {
    data_track_addressbar : true,
    data_ga_property      : 'UA-33396402-5',
    data_ga_social        : true
  };
</script>
<script type="text/javascript" src="//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-4fd7768413d24ce9"></script>
</body>
</html>